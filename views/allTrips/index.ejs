<%- include('../partials/header') %>
<table>
  <thead>
    <tr>
      <th>All Road Trips</th>
    </tr>
  </thead>
  <tbody>
    <!-- need to change details link to go to specific trip id! -->
  <% trips.forEach(function(t) { %>
    <tr>
      <td><a href="/allTrips/<%= t._id %>"><%= t.tripStartEnd %></a></td>
      <td><%= t.tripHours %></td>
      
      <!-- <td><a href="/trips/t._id/new">ADD/EDIT TRIP DETAILS</a></td> -->
    <% }); %>
  </tbody>
</table>
    
  
